{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": ".",
  "builds": [
    {
      "src": "api/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/api/server.js" }
  ],
  "functions": {
    "api/server.js": {
      "maxDuration": 60
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}